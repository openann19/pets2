{
  "name": "pawfectmatch-web",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "dev:demo": "cross-env NEXT_PUBLIC_DEMO_MODE=1 next dev -p 3002",
    "build:demo": "cross-env NEXT_PUBLIC_DEMO_MODE=1 next build",
    "start:demo": "cross-env NEXT_PUBLIC_DEMO_MODE=1 next start -p 3002",
    "typecheck": "tsc -b --pretty false",
    "lint": "eslint src --max-warnings=0",
    "lint:strict": "cross-env ESLINT_STRICT=1 eslint src --max-warnings=0",
    "fix": "eslint src --fix",
    "test": "jest --runInBand",
    "test:web": "jest --runTestsByPath src/components/AI/__tests__ src/__tests__/components || jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "playwright": "playwright test",
    "playwright:ui": "playwright test --ui",
    "playwright:debug": "playwright test --debug",
    "e2e": "start-server-and-test 'pnpm dev' http://localhost:3000 'cypress run'",
    "e2e:open": "start-server-and-test 'pnpm dev' http://localhost:3000 'cypress open'",
    "e2e:playwright": "playwright test",
    "visual": "percy exec -- playwright test tests/visual/",
    "lighthouse-ci": "lhci autorun"
  },
