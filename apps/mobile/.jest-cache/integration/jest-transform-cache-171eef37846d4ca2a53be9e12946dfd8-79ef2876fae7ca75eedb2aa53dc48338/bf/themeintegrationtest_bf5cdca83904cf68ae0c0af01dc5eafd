"use strict";

var _resolve = require("../resolve");
describe('resolved theme', function () {
  test('has ergonomic keys', function () {
    var t = (0, _resolve.getLightTheme)();
    expect(typeof t.colors.primary).toBe('string');
    expect(typeof t.colors.primaryText).toBe('string');
    expect(typeof t.colors.bg).toBe('string');
    expect(typeof t.radius.md).toBe('number');
  });
  test('dark variant flips text/bg', function () {
    var l = (0, _resolve.getLightTheme)();
    var d = (0, _resolve.getDarkTheme)();
    expect(l.colors.bg).not.toBe(d.colors.bg);
    expect(l.colors.text).not.toBe(d.colors.text);
  });
});
